{
  "name": "polymorphic-tests",
  "description": "Polymorphic test framework for JS by Wizard Enterprises.",
  "version": "0.2.0",
  "author": "homunculus@wizard.enterprises",
  "bin": {
    "polytest": "./bin/run"
  },
  "bugs": "https://github.com/wizard-enterprises/wizard-library/issues",
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "clear": "^0.1.0",
    "commander": "^3.0.1",
    "diff": "^4.0.1",
    "figlet": "^1.2.4",
    "fs-extra": "^8.1.0",
    "glob-watcher": "^5.0.3",
    "globby": "^10.0.1",
    "inquirer": "^7.0.0",
    "lodash": "^4.17.15",
    "path": "^0.12.7",
    "pjson": "^1.0.9",
    "rxjs": "^6.5.3",
    "source-map-support": "^0.5.13",
    "stream-to-string": "^1.2.0",
    "tap-mocha-reporter": "^5.0.0",
    "tslib": "^1.10.0",
    "wizard-decorators": "^1.0.3",
    "wizard-utils": "^0.0.4",
    "yaml": "^1.7.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^8.0.0",
    "@types/node": "^12.12.8",
    "chai-shallow-deep-equal": "^1.4.6",
    "ts-node": "^8.5.2",
    "typescript": "^3.3"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/wizard-enterprises/wizard-library/blob/master/packages/polymorphic-tests",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "./lib",
  "oclif": {
    "commands": "./lib/cli/commands",
    "bin": "polytest",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wizard-enterprises/wizard-library/blob/master/packages/polymorphic-tests"
  },
  "scripts": {
    "build": "rm -rf lib && tsc -p ./tsconfig.json",
    "build:polytest": "rm -rf lib && tsc -p ./tsconfig.polytest.json",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepare": "npm run build",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "start": "ts-node src/cli.ts",
    "pretest": "npm run build",
    "test": "./bin/run run",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
