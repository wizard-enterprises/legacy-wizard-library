{
  "sourceMaps": "both",
  "presets": [
    ["@babel/preset-env", { "targets": ["current node", { "esmodules": true }] }]
  ],
  "plugins": [
    "@babel/plugin-proposal-nullish-coalescing-operator",
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ]
}